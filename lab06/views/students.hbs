<table border="1">
    <tr>
        <th>Student List</th>
    </tr>
    <tr>
        <th>Name</th>
        <th>DOB</th>
        <th>Gender</th>
        <th>GPA</th>
    </tr>

    {{#each students }}
        <tr>
            <th>{{name}}</th>
            <th>{{dateFormat dob "DD, MM YYYY"}} </th>
            <th>{{gender}}</th>
            <th>{{gpa}}</th>
        </tr>
    {{/each}}
</table>